<?xml version="1.0"?>
<configuration>
  <configSections>
    <section name="superSocket" type="SuperSocket.SocketEngine.Configuration.SocketServiceConfig, SuperSocket.SocketEngine"/>
  </configSections>
  <appSettings>
    <add key="ServiceName" value="ScadaService"/>
    <!--Scada转发服务器IP-->
    <add key="ScadaServerIP" value="10.181.48.138"/>
    <!--Scada转发服务器端口-->
    <add key="ScadaServerPort" value="8866"/>
  </appSettings>
  <superSocket>
    <servers>
      <server name="ScadaServer" serverTypeName="ScadaService" ip="Any" port="2012" maxConnectionNumber="1000">
      </server>
    </servers>
    <serverTypes>
      <add name="ScadaService" type="PR.WizPlant.ScadaMono.ScadaServer, PR.WizPlant.ScadaMono"/>
    </serverTypes>
  </superSocket>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/>
  </startup>
  <runtime>
    <gcServer enabled="true"/>
  </runtime>
</configuration>
